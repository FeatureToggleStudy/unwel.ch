{
  "name": "@unwelch/backend",
  "version": "0.0.1",
  "license": "GPL-3.0-only",
  "main": "src/server.js",
  "scripts": {
    "build": "webpack",
    "prod": "npm run migrate && npm run serve",
    "serve": "NODE_ENV=production node lib/server.js",
    "dev": "npm run cleandb && concurrently -k \"npm run startdb\" \"babel-node wait-for-db.js && npm run migrate && npm run start\"",
    "start": "nodemon src/server.js --exec babel-node --inspect",
    "startdb": "docker run --name unwelch-db -p 5434:5432 -e POSTGRES_PASSWORD=postgres postgres:10.7",
    "cleandb": "docker stop unwelch-db || true && docker rm unwelch-db || true",
    "migrate": "babel-node ./migrate.js",
    "test": "jest src/",
    "test:watch": "jest src/ --watch"
  },
  "dependencies": {
    "@sentry/node": "4.5.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "glob-require-dir": "^1.0.3",
    "graphql": "^0.13.1",
    "hoek": "^6.1.2",
    "husky": "^0.14.3",
    "jsonwebtoken": "^7.4.3",
    "lint-staged": "^6.0.0",
    "merge": "^1.2.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.3.0",
    "ramda": "^0.25.0",
    "sanctuary": "^0.13.2",
    "sqlstring": "^2.3.0",
    "squel": "^5.12.0",
    "web-push": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "jest": "^21.2.1",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.10.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2"
  }
}
